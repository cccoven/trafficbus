{
    "ipSets": [
        {
            "name": "myset",
            "addrs": [
                "127.0.0.1/8",
                "192.168.0.10"
            ]
        }
    ],
    "ruleSets": [
        {
            "iface": "lo",
            "rules": [
                {
                    "num": 1,
                    "target": "DROP",
                    "protocol": "ICMP",
                    "source": "",
                    "destination": "",
                    "matchExtension": {}
                },
                {
                    "num": 2,
                    "target": "ACCEPT",
                    "protocol": "TCP",
                    "source": "0.0.0.0/0",
                    "destination": "0.0.0.0",
                    "matchExtension": {
                        "enable": 1,
                        "set": {
                            "name": "myset",
                            "direction": "DST"
                        },
                        "tcp": {
                            "dstPort": 8080
                        }
                    }
                },
                {
                    "num": 3,
                    "target": "ACCEPT",
                    "protocol": "UDP",
                    "source": "0.0.0.0/0",
                    "destination": "0.0.0.0",
                    "matchExtension": {
                        "enable": 1,
                        "udp": {
                            "dstPort": 8081
                        }
                    }
                }
            ]
        }
    ]
}